      <Button fx:id="addCertificateButton" layoutX="147.0" layoutY="113.0" mnemonicParsing="false" onAction="#addCertificateButtonClicked" prefHeight="53.0" prefWidth="137.0" text="DODAJ SERTIFIKAT" />
      <Button fx:id="logoutButton" layoutX="147.0" layoutY="250.0" mnemonicParsing="false" onAction="#logoutButtonClicked" prefHeight="53.0" prefWidth="137.0" text="ODJAVI SE" />
